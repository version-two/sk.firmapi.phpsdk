{
    "name": "firmapi/phpsdk",
    "description": "Official PHP SDK for FirmAPI - Slovak Company Data API",
    "type": "library",
    "license": "MIT",
    "keywords": ["firmapi", "api", "sdk", "slovak", "companies", "orsr", "business-register"],
    "authors": [
        {
            "name": "Version Two",
            "email": "info@versiontwo.sk"
        }
    ],
    "homepage": "https://firmapi.sk",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "FirmApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FirmApi\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "illuminate/support": "Required for Laravel integration (^10.0|^11.0|^12.0)"
    },
    "extra": {
        "laravel": {
            "providers": [
                "FirmApi\\Laravel\\FirmApiServiceProvider"
            ],
            "aliases": {
                "FirmApi": "FirmApi\\Laravel\\Facades\\FirmApi"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
